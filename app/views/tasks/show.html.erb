<h1>Info of Task</h1>

<p>Title: <%= @task.title %></p>
<p>Details: <%= @task.details %></p>
<p>Completed: <%= @task.completed %></p>

<p><%= link_to 'All tasks', tasks_path %></p>
<p><%#= link_to 'All tasks', _task_path %></p>
<p><%= link_to 'Add new task', new_task_path %></p>
<p><%= link_to 'Edit this task', edit_task_path(@task) %></p>
<p><%= link_to 'Destroy task', task_path(@task), method: :delete, data: { confirm: 'Are you sure?' } %></p>

